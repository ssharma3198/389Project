


<script src="http://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js" type="text/javascript"></script>
<div class="wrap">
  <div class="feedback">
    <h1>Feedback Form</h1>
    <form action="">
      <div>
        <input type="text" id="user" required>
        <label for="text">User</label>
      </div>
      <div>
        <input type="text" id="book" required>
        <label for="Book" >Book review</label>
      </div>
      <div>
        <input type="text" id="author" required>
        <label for="Author" >Author</label>
      </div>
      <div>
        <input type="text" id="genre">
        <label for="Genre" >Genres (Comma Seperated)</label>
      </div>
      <div>
        <input type="text" id="rating" required>
        <label for="text">Rating (Out of 5) </label>
      </div>
      <div>
        <textarea name="message" id="comment" cols="30" rows="5" required></textarea>
        <label for="message" >message</label>
      </div>
      <div >
        <div>
        	<button type="submit", id="submit_button", onclick="save_record()")>Submit</button>        
        </div>
      </div>
    </form>
  </div>
</div>

<script src="/JS/data-util.js" type="text/javascript" ></script>

